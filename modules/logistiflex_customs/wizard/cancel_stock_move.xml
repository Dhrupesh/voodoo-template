<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_cancel_stock_move_wizard" model="ir.ui.view">
            <field name="name">cancel.stock.move.view</field>
            <field name="model">cancel.stock.move</field>
            <field name="arch" type="xml">
                <form string="Cancel Stock Move" version="7.0">
                    <field name="comment" nolabel="1"/>
                    <footer>
                        <button name="cancel_move" string="Confirm cancel" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_cancel_stock_move_wizard" model="ir.actions.act_window">
            <field name="name">Cancel Stock Move</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cancel.stock.move</field>
            <field name="view_type">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_cancel_stock_move_wizard"/>
        </record>

    </data>
</openerp>

